{
  "name": "react-table",
  "version": "6.11.5",
  "description": "A fast, lightweight, opinionated table and datagrid built on React",
  "license": "MIT",
  "homepage": "https://github.com/tannerlinsley/react-table/tree/v6#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tannerlinsley/react-table.git"
  },
  "keywords": [
    "react",
    "table",
    "react-table",
    "datagrid"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "src/",
    "es/",
    "lib/",
    "react-table.js",
    "react-table.min.js",
    "react-table.css",
    "media/*.png"
  ],
  "scripts": {
    "build:cjs": "babel src --out-dir lib --source-maps inline --copy-files",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es --source-maps inline",
    "build:umd": "cross-env BABEL_ENV=umd rollup -c",
    "build:umd-min": "cross-env BABEL_ENV=umd NODE_ENV=production rollup -c",
    "build:css": "rimraf react-table.css && stylus src/index.styl --compress -o react-table.css && npm run css:autoprefix",
    "css:autoprefix": "postcss *.css --use autoprefixer -r",
    "watch": "npm-run-all --parallel watch:*",
    "watch:js": "onchange 'src/**/*.js' -i -- yarn build:cjs",
    "watch:css": "onchange 'src/**/*.styl' -i -- yarn build:css",
    "test": "eslint src",
    "build": "npm-run-all build:*",
    "prepare": "npm run build",
    "docs": "yarn watch & cd docs && yarn && yarn start",
    "docs:build": "yarn build && cd docs && yarn && yarn build"
  },
  "dependencies": {
    "@types/react-table": "^6.8.15",
    "classnames": "^2.5.1",
    "react-is": "^18.3.1"
  },
  "peerDependencies": {
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.7",
    "@babel/eslint-parser": "^7.26.5",
    "@babel/plugin-external-helpers": "^7.25.9",
    "@babel/plugin-transform-class-properties": "^7.25.9",
    "@babel/plugin-transform-object-rest-spread": "^7.25.9",
    "@babel/preset-env": "^7.26.7",
    "@babel/preset-react": "^7.26.3",
    "@babel/runtime": "^7.26.7",
    "@blaumaus/rollup-plugin-uglify": "^7.0.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-replace": "^6.0.2",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-class-property": "^1.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-react": "^7.37.4",
    "match-sorter": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "postcss": "^8.5.1",
    "postcss-cli": "^11.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-json-tree": "^0.19.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.32.1",
    "standard": "^17.1.2",
    "stylus": "^0.64.0"
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "modules": "commonjs"
            }
          ],
          "@babel/preset-react"
        ],
        "plugins": [
          "@babel/plugin-transform-object-rest-spread",
          "@babel/plugin-transform-class-properties"
        ]
      },
      "es": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "modules": false
            }
          ],
          "@babel/preset-react"
        ],
        "plugins": [
          "@babel/plugin-transform-object-rest-spread",
          "@babel/plugin-transform-class-properties"
        ]
      },
      "umd": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "modules": false
            }
          ],
          "@babel/preset-react"
        ],
        "plugins": [
          "@babel/plugin-transform-object-rest-spread",
          "@babel/plugin-transform-class-properties"
        ]
      }
    }
  }
}
